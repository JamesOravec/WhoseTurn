<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
              http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5" xmlns="http://java.sun.com/xml/ns/javaee">

	<!-- Servlets -->
	<servlet>
		<servlet-name>greetServlet</servlet-name>
		<servlet-class>edu.unlv.cs673.whoseturn.server.GreetingServiceImpl</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>greetServlet</servlet-name>
		<url-pattern>/whoseturn/greet</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>guestbook</servlet-name>
		<servlet-class>guestbook.GuestbookServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>guestbook</servlet-name>
		<url-pattern>/guestbook</url-pattern>
	</servlet-mapping>

	<!-- Example from: http://www.fishbonecloud.com/2010/11/use-objectify-to-store-data-in-google.html -->
	<servlet>
		<servlet-name>persistenceServlet</servlet-name>
		<servlet-class>edu.unlv.cs673.whoseturn.server.PersistentServiceImpl</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>persistenceServlet</servlet-name>
		<url-pattern>/objectifyexample/persistentservice</url-pattern>
	</servlet-mapping>


	<!-- Default page to serve -->
	<welcome-file-list>
		<welcome-file>WhoseTurn.html</welcome-file>
	</welcome-file-list>

</web-app>
